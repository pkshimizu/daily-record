{
    "dart.flutterSdkPath": "flutter",
    "dart.sdkPath": "flutter",
    "dart.debugExternalPackageLibraries": false,
    "dart.debugSdkLibraries": false,
    "dart.showInspectorNotificationsForWidgetErrors": true,
    "dart.previewFlutterUiGuides": true,
    "dart.previewFlutterUiGuidesCustomTracking": true,
    "dart.closingLabels": true,
    "dart.lineLength": 80,
    "dart.enableSdkFormatter": true,
    "dart.formatOnSave": true,
    "dart.formatOnType": true,
    "dart.showOutline": true,
    "dart.showErrors": true,
    "dart.showWarnings": true,
    "dart.showHints": true,
    "dart.showInfo": true,
    "dart.analyzerPath": "flutter",
    "dart.checkForSdkUpdates": false,
    "dart.analysisServerFolding": true,
    "dart.analysisServerSnippets": true,
    "dart.completeFunctionCalls": true,
    "dart.showTodos": true,
    "dart.showLintNames": true,
    "dart.showLintSeverity": true,
    "dart.analyzerDiagnosticsPort": null,
    "dart.analyzerObservatoryPort": null,
    "dart.analyzerLogFile": null,
    "dart.analyzerAdditionalArgs": [],
    "dart.analyzerVmArgs": [],
    "editor.formatOnSave": true,
    "editor.formatOnType": true,
    "editor.codeActionsOnSave": {
        "source.fixAll": "explicit",
        "source.organizeImports": "explicit"
    },
    "files.trimTrailingWhitespace": true,
    "files.insertFinalNewline": true,
    "files.trimFinalNewlines": true
} 